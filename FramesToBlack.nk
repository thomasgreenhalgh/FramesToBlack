set cut_paste_input [stack 0]
version 14.1 v4
push $cut_paste_input
Group {
 name FramesToBlack
 knobChanged "import re\n\ndef checkKnobChanged():\n	# Get the input from 'framesToSwitchOff' knob and split by spaces\n	inputFrames = nuke.thisNode()\['framesToSwitchOff'].value().split(' ')\n	multiplyNode = nuke.toNode('Multiply1')  # Ensure 'Multiply1' exists\n	frames = \[]\n\n	regexFrameRange = re.compile(r\"(\\d+)-(\\d+)\")\n\n	expr_parts = \[]  # To hold the ramp expression parts\n	frame_expr_parts = \[]  # To hold the frame switch off parts\n\n	# Debugging information\n	print(f\"Input framesToSwitchOff: \{inputFrames\}\")\n\n	if '*' in inputFrames:\n    	expr = \"0\"  # Set multiply to 0, switching all frames off\n    	print(\"Detected '*', setting expression to 0.\")\n	elif inputFrames and any(inputFrames):  # Check if inputFrames has any non-empty values\n    	for x in inputFrames:\n        	print(f\"Processing input: \{x\}\")\n        	if '-' in x:  # Process frame range like 1020-1030\n            	match = regexFrameRange.match(x)\n            	if match:\n                	start, end = sorted(map(int, match.groups()))  # Extract start and end frames\n                	frames.extend(range(start, end + 1))  # Add all frames in the range\n                	print(f\"Matched range: \{start\}-\{end\}, adding frames \{list(range(start, end + 1))\}\")\n        	elif x.isnumeric():  # Process single frame like 1040\n            	frames.append(int(x))\n            	print(f\"Matched single frame: \{x\}\")\n        	elif '>' in x or '<' in x:  # Handle ramp conditions\n            	if '>' in x:\n                	start, end = map(int, x.split('>'))  # Extract the start and end frames\n                	# Ramp from 1 to 0\n                	expr_parts.append(f\"(t>=\{start\} && t<=\{end\}) ? (1 - ((t-\{start\})/(\{end\}-\{start\})))\")\n                	print(f\"Matched ramp down: \{start\}>\{end\}\")\n            	elif '<' in x:\n                	start, end = map(int, x.split('<'))  # Extract the start and end frames\n                	# Ramp from 0 to 1\n                	expr_parts.append(f\"(t>=\{start\} && t<=\{end\}) ? ((t-\{start\})/(\{end\}-\{start\}))\")\n                	print(f\"Matched ramp up: \{start\}<\{end\}\")\n\n    	frames = sorted(set(frames))\n    	print(f\"Processed frames to switch off: \{frames\}\")\n    	\n    	# Handle individual frames or frame ranges turning off\n    	if frames:\n        	frame_expr_parts = \[f\"(t==\{f\}) ? 0\" for f in frames]\n\n    	# Combine the ramp expressions with the frame-based expressions\n    	# Only add the default 1 if we have expressions in expr_parts or frame_expr_parts\n    	all_expr_parts = expr_parts + frame_expr_parts\n    	if all_expr_parts:\n        	expr = ' : '.join(all_expr_parts) + \" : 1\"  # Ensure default to 1 at the end\n    	else:\n        	expr = \"1\"  # Default to 1 if no input matches\n\n    	print(f\"Constructed expression: \{expr\}\")\n	else:\n    	expr = \"1\"  # If no frames are given, keep the Multiply node at 1\n    	print(\"No valid input detected, defaulting expression to 1.\")\n\n	# Apply the expression to the Multiply node\n	print(f\"Applying expression to Multiply1 node: \{expr\}\")\n	multiplyNode\['value'].setExpression(expr)\n\n\n# This checks if the knob that was changed is 'framesToSwitchOff'\nif 'framesToSwitchOff':\n	checkKnobChanged()\nelse:\n	print(\"No change detected in 'framesToSwitchOff' knob.\")\n"
 label "\[if \{\[value framesToSwitchOff] == \"\"\} \{return \"\"\} \{return \"\[value framesToSwitchOff]\"\}]\n\[if \{\[value framesToSwitchOff] == \"\"\} \{return \[knob tile_color 0]\} \{return \[knob tile_color 0x55ff7fff]\}]\n\n"
 note_font "Bitstream Vera Sans Bold"
 note_font_size 15
 selected true
 xpos 144
 ypos -36
 addUserKnob {20 User l -}
 addUserKnob {26 FramesToBlack l "" +STARTLINE T "<img src=\"data:image/png;base64,\" />"}
 addUserKnob {26 ""}
 addUserKnob {1 framesToSwitchOff l "" +STARTLINE}
 addUserKnob {22 frameRange l "Set Frame Range" -STARTLINE T "f = nuke.thisNode()\['framesToSwitchOff']\nfirst = int(nuke.Root()\['first_frame'].getValue())\nlast = int(nuke.Root()\['last_frame'].getValue())\nv = str(first)+'-'+str(last)\nf.setValue(v)\n"}
 addUserKnob {22 turnOff l "All Frames To Black" -STARTLINE T "f = nuke.thisNode()\['framesToSwitchOff']\nf.setValue('*')"}
 addUserKnob {26 ""}
 addUserKnob {26 Version l "" +STARTLINE T "<div style=\"text-align: left; font-size: 10px; color: #666666; font-style: italic;\">\n  v 1.5 : 2024-10\n</div>"}
 addUserKnob {20 Instructions}
 addUserKnob {26 text_5 l "" -STARTLINE T "Fill in the field \"Frames That You Want To Be Black\" with frame numbers or \nframe ranges and the image will go black on those specific frames.\n\nA single frame is just a number.  \nFor example \"993\"\n\nA frame range is represented by a number a dash then another number.  \nFor example \"995-1001\"\n\nYou can even type \"1001<1010\" to fade on between these frames\n\nPut an \"*\"(asterix) to set all frames to black\n\nExample input \"1001>1010 1014 1020-1030\""}
}
 Input {
  inputs 0
  name Inputin
  xpos -325
  ypos 310
 }
 Multiply {
  value {{1}}
  name Multiply1
  tile_color 0x6aff55ff
  xpos -325
  ypos 350
 }
 Output {
  name Output1
  xpos -325
  ypos 450
 }
end_group
